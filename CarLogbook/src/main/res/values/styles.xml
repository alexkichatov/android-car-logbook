        <item name="android:spinnerDropDownItemStyle">@style/App.Widget.DropDownItem.Spinner</item>
        <item name="spinnerDropDownItemStyle">@style/App.Widget.DropDownItem.Spinner</item>
    </style>

	<style name="Test" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar">
		<item name="android:spinnerDropDownItemStyle">@style/App.Widget.DropDownItem.Spinner</item>
