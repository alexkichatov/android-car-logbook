		<item name="colorAccent">@color/colorAccent</item>

		<item name="windowActionBar">false</item>
		<item name="windowActionBarOverlay">true</item>

		<item name="android:windowActionBar" tools:ignore="NewApi">false</item>
		<item name="android:windowActionBarOverlay" tools:ignore="NewApi">true</item>

		<!--<item name="android:spinnerItemStyle">@style/App.Widget.Item.Spinner</item>-->
		<!--<item name="android:spinnerDropDownItemStyle">@style/dropDownSpinnerItem</item>-->
  </style>

	<style name="selector" parent="@style/ThemeOverlay.AppCompat.Light">
		<item name="android:spinnerItemStyle">@style/App.Widget.Item.Spinner</item>
		<item name="android:spinnerDropDownItemStyle">@style/dropDownSpinnerItem</item>
	</style>


	<style name="card" parent="wide">
		<!--<item name="android:foreground">?android:attr/selectableItemBackground</item>-->
